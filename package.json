{
  "name": "multilingual-dict",
  "version": "1.0.0",
  "description": "This is multilingual directory app like google translate",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "prisma:generate": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "seed": "ts-node --transpile-only scripts/seed.ts",
    "migrate": "ts-node --transpile-only scripts/migrate.ts",
    "create-indexes": "ts-node --transpile-only scripts/create-indexes.ts",
    "bench": "ts-node --transpile-only bench/queries.ts",
    "db:indexes": "ts-node scripts/create-indexes.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.10.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "prisma": "^6.19.0"
  }
}
